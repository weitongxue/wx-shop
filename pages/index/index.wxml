<!--index.wxml-->
<view class="container">
  <view class="header">
    <image class="logo" src="../../image/tab/icon-logo.png"></image>
    <input class="search" placeholder="请输入商品名称" style="background: white url('../../image/tab/icon-search.png') 30rpx 18rpx no-repeat; background-size: 40rpx;"></input>
  </view>
  <!-- 轮播图 -->
  <view class="swiper-wrap">
    <mSwiper imageList="{{imageList}}" />
  </view>
  <!-- 分类 -->
  <view class="nav-wrap">
    <mNav navList="{{navList}}" />
  </view>
  <!-- 公告 -->
  <view class="notice-wrap">
    <mNotice noticeList="{{noticeList}}" icon="{{noticeIcon}}" />
  </view>
  <!-- 商品列表 -->
  <view class="product-wrap">
    <mProduct infoList="{{infoList}}" productList="{{productList}}" bindhandleProduct="getProduct" />
  </view>
</view>
